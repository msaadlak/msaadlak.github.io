#################################################################
#    Object Properties
#################################################################

###  http://qudt.org/2.1/schema/qudt/hasQuantityValue
qudt:hasQuantityValue rdf:type owl:ObjectProperty ;
                      rdfs:domain qudt:Quantity ;
                      rdfs:range qudt:QuantityValue ;
                      rdf:type owl:AsymmetricProperty ,
                               owl:ObjectProperty ;
                      rdfs:label "has quantity value"@en .


###  http://qudt.org/2.1/schema/qudt/hasUnit
qudt:hasUnit rdf:type owl:ObjectProperty ;
             rdfs:domain qudt:Quantity ;
             rdfs:range qudt:Unit ;
             rdf:type owl:ObjectProperty ;
             rdfs:label "has unit"@en .


###  https://schema.org/author
<https://schema.org/author> rdf:type owl:ObjectProperty ;
                           rdfs:domain <https://schema.org/Recipe> ;
                           rdfs:range <https://schema.org/Person> ;
                           rdf:type owl:ObjectProperty ;
                           rdfs:label "author"@en .


###  https://schema.org/cookTime
<https://schema.org/cookTime> rdf:type owl:ObjectProperty ;
                             rdfs:domain <https://schema.org/Recipe> ;
                             rdfs:range qudt:Quantity ;
                             rdf:type owl:ObjectProperty ;
                             rdfs:label "cook time"@en .


###  https://schema.org/hasPart
<https://schema.org/hasPart> rdf:type owl:ObjectProperty ;
                            rdfs:domain <https://schema.org/Recipe> ;
                            rdfs:range [ rdf:type owl:Class ;
                                         owl:unionOf ( :Ingredient
                                                       :Procedure
                                                     )
                                       ] ;
                            rdf:type owl:ObjectProperty .


###  https://schema.org/nutrition
<https://schema.org/nutrition> rdf:type owl:ObjectProperty ;
                              rdfs:domain <https://schema.org/Recipe> ;
                              rdfs:range <https://schema.org/NutritionInformation> ;
                              rdf:type owl:ObjectProperty ;
                              rdfs:label "nutrition"@en .


###  https://schema.org/prepTime
<https://schema.org/prepTime> rdf:type owl:ObjectProperty ;
                             rdfs:domain <https://schema.org/Recipe> ;
                             rdfs:range qudt:Quantity ;
                             rdf:type owl:ObjectProperty ;
                             rdfs:label "prep time"@en .


###  https://schema.org/totalTime
<https://schema.org/totalTime> rdf:type owl:ObjectProperty ;
                              rdfs:domain <https://schema.org/Recipe> ;
                              rdfs:range qudt:Quantity ;
                              rdf:type owl:ObjectProperty ;
                              rdfs:label "total time"@en .


###  https://schema.org/yield
<https://schema.org/yield> rdf:type owl:ObjectProperty ;
                          rdfs:domain <https://schema.org/Recipe> ;
                          rdfs:range qudt:Quantity ;
                          rdf:type owl:ObjectProperty ;
                          rdfs:label "yield"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyFollows
seq:directlyFollows rdf:type owl:ObjectProperty ;
                    owl:inverseOf seq:directlyPrecedes ;
                    rdfs:domain owl:Action ;
                    rdfs:range owl:Action ;
                    rdf:type owl:ObjectProperty ;
                    rdfs:label "directily follows"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#directlyPrecedes
seq:directlyPrecedes rdf:type owl:ObjectProperty ;
                     rdfs:domain owl:Action ;
                     rdfs:range owl:Action ;
                     rdf:type owl:ObjectProperty ;
                     rdfs:label "directly precedes"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#follows
seq:follows rdf:type owl:ObjectProperty ;
            owl:inverseOf seq:precedes ;
            rdfs:domain owl:Action ;
            rdfs:range owl:Action ;
            owl:propertyChainAxiom ( seq:directlyFollows
                                     seq:directlyFollows
                                   ) ;
            rdf:type owl:ObjectProperty ;
            rdfs:label "follows"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes
seq:precedes rdf:type owl:ObjectProperty ;
             rdfs:domain owl:Action ;
             rdfs:range owl:Action ;
             owl:propertyChainAxiom ( seq:directlyPrecedes
                                      seq:directlyPrecedes
                                    ) ;
             rdf:type owl:ObjectProperty ;
             rdfs:label "precedes"@en .


###  http://www.w3.org/2002/07/owl#has_duration
owl:has_duration rdf:type owl:ObjectProperty ;
                 rdfs:domain owl:Action ;
                 rdfs:range qudt:Quantity ;
                 rdf:type owl:ObjectProperty ;
                 rdfs:label "has duration"@en .


###  https://raw.githubusercontent.com/msaadlak/msaadlak.github.io/main/EvoRecipes/evo.owl#alternateAction
:alternateAction rdf:type owl:ObjectProperty ;
                 rdfs:domain owl:Action ;
                 rdfs:range owl:Action ;
                 rdf:type owl:ObjectProperty ,
                          owl:SymmetricProperty ;
                 rdfs:label "Alternate Action"@en .


###  https://raw.githubusercontent.com/msaadlak/msaadlak.github.io/main/EvoRecipes/evo.owl#directlyFollowsProcedure
:directlyFollowsProcedure rdf:type owl:ObjectProperty ;
                          owl:inverseOf :directlyPrecedesProcedure ;
                          rdfs:domain :Procedure ;
                          rdfs:range :Procedure ;
                          rdf:type owl:ObjectProperty ;
                          rdfs:label "directly follows procedures"@en .


###  https://raw.githubusercontent.com/msaadlak/msaadlak.github.io/main/EvoRecipes/evo.owl#directlyPrecedesProcedure
:directlyPrecedesProcedure rdf:type owl:ObjectProperty ;
                           rdfs:domain :Procedure ;
                           rdfs:range :Procedure ;
                           rdf:type owl:ObjectProperty ;
                           rdfs:label "directly precedes procedure"@en .


###  https://raw.githubusercontent.com/msaadlak/msaadlak.github.io/main/EvoRecipes/evo.owl#hasAction
:hasAction rdf:type owl:ObjectProperty ;
