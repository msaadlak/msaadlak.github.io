                                        owl:versionIRI <https://raw.githubusercontent.com/msaadlak/msaadlak.github.io/main/EvoRecipes/evo.owl#v/1.0> ;
                                        dc:publisher "Recipe Evolution Group" ;
                                        dc:title "RecipeOn Ontology" ;
                                        terms:contributor <https://hajirajabeen.github.io> ,
